<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <Nullable>enable</Nullable>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>AnyBase.Net</Title>
        <Authors>Polecola</Authors>
        <Description>Helper to convert strings from to any base</Description>
        <Copyright>MIT</Copyright>
        <PackageProjectUrl>https://github.com/MiLattanzio/AnyBase.Net</PackageProjectUrl>
        <PackageLicenseUrl>https://github.com/MiLattanzio/AnyBase.Net/blob/master/LICENSE</PackageLicenseUrl>
        <RepositoryUrl>https://github.com/MiLattanzio/AnyBase.Net</RepositoryUrl>
        <AssemblyVersion>1.0.1</AssemblyVersion>
        <FileVersion>1.0.1</FileVersion>
        <Version>1.0.1</Version>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="NumeralSystems.Net" Version="4.5.1" />
    </ItemGroup>

</Project>
